
#!/usr/bin/env ruby
$LOAD_PATH.unshift File.expand_path('../../lib', __FILE__)

require 'ceie'

puts "\n" + "*" * 80  + "\n" +
     " " * 20 + "Enjambre de Particulas" + " " * 20 + "\n" +
     "*" * 80 + "\n"


rangox = [-100.0,100.0]
rangoy = [-100.0,100.0]
enjambre = Ceie::Enjambre.new(50,rangox,rangoy,1000,3)
enjambre.resolver


